local function notify(text)
    Fluent:Notify({
        Title = "KadeHub",
        Content = text,
        Duration = 10
    })
end

Tabs.Main:AddButton({
    Title = "Lock On",
    Description = "Mouse Button 3 to lock on to somebody. (Credits To Reap)",
    Callback = function()
        local v = Instance.new('StringValue')
        v.Parent = game.Workspace
        v.Name = 'Lock'
        _G.LockTo = nil
        local plr = game.Players.LocalPlayer
        local chr = plr.Character
        local V3 = Vector3.new
        local CF = CFrame.new
        local UIS = game:GetService('UserInputService')

        local function GetTarget(T)
            if T then
                local T = T
                for i = 1, 20 do 
                    T = T.Parent 
                    if T == workspace then 
                        return nil 
                    elseif T:FindFirstChildOfClass('Humanoid') then
                        return T
                    end 
                end 
            end
        end

        pcall(function() 
            if _G.ke then 
                _G.ke:Disconnect() 
            end 
        end)

        local Locked = nil
        local alignOrientation = nil
        _G.ke = UIS.InputBegan:Connect(function(k, ch)
            if ch then return end
            if k.UserInputType == Enum.UserInputType.MouseButton3 then 
                local m = plr:GetMouse()
                local T = GetTarget(m.Target)
                if T and T:FindFirstChild('HumanoidRootPart') and not Locked then 
                    Locked = T
                    print('Locking onto..'..Locked.Name)
                    _G.LockTo = Locked
                    for _, v in pairs(chr:GetDescendants()) do 
                        if v.Name == 'LockOnClass' then 
                            v:Destroy() 
                        end
                    end 
                    local hrp = chr.HumanoidRootPart
                    local p = Instance.new('Part', chr)
                    p.Name = 'LockOnClass'
                    p.CanCollide = false
                    p.Anchored = true 
                    p.CFrame = hrp.CFrame
                    p.Transparency = 1 
                    local at = Instance.new('Attachment', hrp)
                    at.Name = 'LockOnClass'
                    local at1 = at:Clone()
                    at1.Parent = p
                    local alignOrientation = Instance.new('AlignOrientation', p)
                    alignOrientation.Attachment0 = nil
                    alignOrientation.Attachment1 = at1
                    alignOrientation.Name = at.Name 
                    alignOrientation.MaxTorque = math.huge
                    alignOrientation.Responsiveness = 200

                    while _G.ke.Connected and Locked and Locked:IsDescendantOf(workspace) and hrp:IsDescendantOf(workspace) do
                        alignOrientation.Attachment0 = at
                        pcall(function()
                            v.Value = Locked.Name
                            local lpos = Locked.HumanoidRootPart.Position
                            local pos = (lpos * V3(1, 0, 1)) + V3(0, p.Position.Y, 0)
                            local E = (lpos - hrp.Position)
                            p.CFrame = CF(hrp.Position, pos)
                            local cam = workspace.CurrentCamera
                            cam.CFrame = CF(cam.CFrame.p, pos)
                            local H = chr.Humanoid
                            H.CameraOffset = V3(2, 0, math.clamp(E.magnitude / 5, 0, 50))
                            -- Update HRP rotation to face the target
                            hrp.CFrame = CF(hrp.Position, lpos)
                        end)
                        game:GetService('RunService').Heartbeat:wait()
                    end 
                    Locked = nil
                    v.Value = 'None'
                    _G.LockTo = nil
                    print('Unlocked.')
                    alignOrientation:Destroy()
                    local H = chr.Humanoid
                    H.CameraOffset = V3(0, 0, 0)
                else
                    Locked = nil
                end 
            end
        end)
    end
})

Tabs.Main:AddButton({
    Title = "Manual Void Kill",
    Description = "Press R to go to the void while grabbing your opponent",
    Callback = function()
        local uis = game:GetService("UserInputService")

        uis.InputBegan:Connect(function(inputs, event)
            if event == true then return end
            if inputs.KeyCode == Enum.KeyCode.R then
                workspace.FallenPartsDestroyHeight = "-nan"
                local oldcframe = game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame
                local rootPart = game.Players.LocalPlayer.Character.HumanoidRootPart
                rootPart.CFrame = CFrame.new(-999999999999999999,-999999999999999999,-999999999999999999)
                wait(4)
                rootPart.CFrame = oldcframe
            end
        end)
    end
})

Tabs.Main:AddButton({
    Title = "Invisible Block",
    Description = "A green aura will appear around you when you invisibly block.",
    Callback = function()
        func=function(v,v2,v3,v4)  -- credits king dream!!!!!
            local ani = Instance.new(v)
            ani[v2] = v3
            if v == "Animation" then
                local loadn = game.Players.LocalPlayer.Character.Humanoid:LoadAnimation(ani)
                return loadn
            else
                ani.Parent = v4
                return ani
            end
        end

        func2=function(args,args2)
            task.spawn(function()
                while task.wait() and game.Players.LocalPlayer.Character do
                    for i, track in pairs(game.Players.LocalPlayer.Character.Humanoid:GetPlayingAnimationTracks()) do
                        if args[track.Animation.AnimationId] then
                            track:Stop()
                            args[track.Animation.AnimationId]:Play()
                        end
                    end
                end
            end)
        end

        local id1 = func("Animation","AnimationId","rbxassetid://7815618175") -- anim id u wanna change it to
        func2({["rbxassetid://10470389827"] = id1}) -- you make an argument of the animation id you wanna replace and then whatever it equals to will be the new outputted animation
        spawn(function()
            game.Players.LocalPlayer.Character.Humanoid:GetPropertyChangedSignal("WalkSpeed"):Connect(function()
                if game.Players.LocalPlayer.Character.Humanoid.WalkSpeed == 8 then
                    game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = 25.1
                end
            end)
            while true do 
                if game.Players.LocalPlayer.Character:GetAttribute("Blocking") == true then
                    for i,v in pairs(game.Players.LocalPlayer.Character.HumanoidRootPart.EsperShield:GetChildren()) do
                        if v:IsA("ParticleEmitter") and not v.Name:match("Impact") then
                            v.Rate = 90
                            v.Enabled = true
                        end
                    end
                elseif game.Players.LocalPlayer.Character:GetAttribute("Blocking") == false then
                    for i,v in pairs(game.Players.LocalPlayer.Character.HumanoidRootPart.EsperShield:GetChildren()) do
                        if v:IsA("ParticleEmitter") and not v.Name:match("Impact") then
                            v.Rate = 90
                            v.Enabled = false
                        end
                    end
                end
                if game.Players.LocalPlayer.Character.Humanoid.MoveDirection.Magnitude <= 0 then
                    for i, track in pairs(game.Players.LocalPlayer.Character.Humanoid:GetPlayingAnimationTracks()) do
                        if track.Animation.AnimationId == "rbxassetid://7815618175" then
                            track:Stop()
                        end
                    end
                end
                task.wait()
            end
        end)
    end
})

