Tabs.Tools:AddButton({
    Title = "20-20-20 Dropkick",
    Description = "Makes you do a walmart version 20-20-20 dropkick from KJ",
    Callback = function()
        local backpack = game.Players.LocalPlayer.Backpack
        local started = false

        if not backpack:FindFirstChild("20-20-20 Dropkick") then
            local NewTool = Instance.new("Tool", game.Players.LocalPlayer)
            NewTool.Name = "20-20-20 Dropkick"
            NewTool.Parent = backpack
        end

        function DisableCollisions()
            for i,v in pairs(workspace.Map.Benchs:GetDescendants()) do
                if v.ClassName == "MeshPart" or v.ClassName == "Part" then
                    v.CanCollide = not v.CanCollide
                end
            end
            for i,v in pairs(workspace.Map.Trees:GetDescendants()) do
                if v.ClassName == "MeshPart" or v.ClassName == "Part" then
                    v.CanCollide = not v.CanCollide
                end
            end
            for i,v in pairs(workspace.Map.Trash:GetDescendants()) do
                if v.ClassName == "MeshPart" or v.ClassName == "Part" then
                    v.CanCollide = not v.CanCollide
                end
            end
        end

        game.Players.LocalPlayer.CharacterAdded:connect(function(v)
            print("who is this kj guy")
            repeat wait() until #game.Players.LocalPlayer.Backpack:GetChildren() <= 4
            wait(0.6)
            local NewTool = Instance.new("Tool", game.Players.LocalPlayer)
            NewTool.Name = "20-20-20 Dropkick"
            NewTool.Parent = game.Players.LocalPlayer.Backpack
        end)

        spawn(function()
            while true do
                if game.Players.LocalPlayer.Character:FindFirstChild("20-20-20 Dropkick") and started == false then
                    local whitestripes = game.ReplicatedStorage.Resources.NinjaRun["Run_Effects"]["White_Stripes"]
                    local kjlines = game.ReplicatedStorage.Resources.KJEffects.speedlines.yeahhhhh
                    local wind = game.ReplicatedStorage.Resources.KJEffects.DropkickExtra.firstHit:GetChildren()[3]:GetChildren()[2].wind
                    local backwind = game.ReplicatedStorage.Resources.NinjaRun["Heavy_Effects"]["Start"]["Teleport_Path"]["End2"]["Back_Wind"]
                    backwind.Rate = 500
                    backwind.Enabled = true
                    wind.Rate = 10000
                    wind.Enabled = true
                    whitestripes.Rate = 500
                    kjlines.Enabled = true
                    kjlines.EmissionDirection = "Back"
                    DisableCollisions()
                    local slowed = Instance.new("Accessory", game.Players.LocalPlayer.Character)
                    slowed.Name = "Slowed"
                    local started = true
                    local phasetwo = false
                    local phasethree = false
                    local pchar = game.Players.LocalPlayer.Character
                    local AnimationId = "13643152947"
                    local Anim = Instance.new("Animation")
                    Anim.AnimationId = "rbxassetid://"..AnimationId
                    local k = pchar:FindFirstChildOfClass('Humanoid'):LoadAnimation(Anim)
                    k:Play()
                    k:AdjustSpeed(0.3)
                    wait(0.5)
                    k:AdjustSpeed(0.1)
                    wait(2)
                    k:AdjustSpeed(1)
                    local Bv = Instance.new("BodyVelocity", game.Players.LocalPlayer.Character.HumanoidRootPart)
                    Bv.P = 1250
                    Bv.MaxForce = Vector3.new(math.huge, 0, math.huge)
                    spawn(function()
                        while true do
                            Bv.Velocity = game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame.lookVector*250
                            task.wait()
                            if phasetwo == true then
                                print('k')
                                break
                            end
                        end
                    end)
                    for i,v in pairs(game.Players.LocalPlayer.Character:GetChildren()) do
                        if v.Name:match("Hitbox_") then
                            local clone = whitestripes:Clone()
                            clone.Parent = v
                            clone.ZOffset = 10
                        end
                    end
                    for i = 1, 2 do
                        local clone = kjlines:Clone()
                        clone.Parent = game.Players.LocalPlayer.Character.HumanoidRootPart
                    end
                    local clone = wind:Clone()
                    clone.Parent = game.Players.LocalPlayer.Character.HumanoidRootPart
                    local clone = backwind:Clone()
                    clone.Parent = game.Players.LocalPlayer.Character.HumanoidRootPart
                    wait(2.2)
                    phasethree = true
                    spawn(function()
                        while true do
                            Bv.Velocity = game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame.lookVector*400
                            task.wait()
                            if phasethree == true then
                                print('j')
                                break
                            end
                        end
                    end)
                    wait(0.8)
                    local Anim = Instance.new("Animation")
                    Anim.AnimationId = "rbxassetid://"..AnimationId
                    local k = pchar:FindFirstChildOfClass('Humanoid'):LoadAnimation(Anim)
                    k:Play()
                    k.TimePosition = 0.8
                    wait(1.5)
                    spawn(function()
                        while true do
                            Bv.Velocity = game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame.lookVector*150
                            task.wait()
                        end
                    end)
                    for i,v in pairs(game.Players.LocalPlayer.Character:GetChildren()) do
                        if v.Name:match("Hitbox_") then
                            v["White_Stripes"]:Destroy()
                        end
                    end
                    for i = 1, 2 do
                        game.Players.LocalPlayer.Character.HumanoidRootPart.yeahhhhh:Destroy()
                    end
                    game.Players.LocalPlayer.Character.HumanoidRootPart.wind:Destroy() 
                    game.Players.LocalPlayer.Character.HumanoidRootPart["Back_Wind"]:Destroy()
                    wait(1)
                    Bv:Destroy()
                    if features.AntiStun == false then
                        game.Players.LocalPlayer.Character:FindFirstChild("Slowed"):Destroy()
                    end
                    local started = false
                    DisableCollisions()
                end
                task.wait()
            end
        end)
    end
})

