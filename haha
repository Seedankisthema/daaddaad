spawn(function()
    local thatsalottawords = {
        "NoBlock",
        --"NoJump",
        "StopRunning",
        "Freeze",
        "Slowed",
        "ComboStun",
        "M1CD",
        "NoDash",
        "NoPunch",
        "UsedDash",
        "Ragdoll",
        "RagdollSim",
        "LaunchLanded",
        "LaunchEnded",
        "NoRotate",
        "BeingGrabbed",
        "NoVel",
        "BeingLaunched",
    }
    while true do
        for i,v in pairs(game.Players.LocalPlayer.Character:GetChildren()) do
            if table.find(thatsalottawords, v.Name) and features.AntiStun == true then
                v:Destroy()
            end
        end
        task.wait()
    end
end)

