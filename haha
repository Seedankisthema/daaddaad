local Toggle = Tabs.Main:AddToggle("AutoCounter", {Title = "Auto Block Counter", Default = false })
Toggle:OnChanged(function(bool)
    features.AutoCounter = bool
end)

spawn(function()
    local lmbs = {
        -- Saitama M1s
        "rbxassetid://10469493270",
        "rbxassetid://10469630950",
        "rbxassetid://10469639222",
        "rbxassetid://10469643643",
        -- Genos M1s
        "rbxassetid://13491635433",
        "rbxassetid://13296577783",
        "rbxassetid://13295919399",
        "rbxassetid://13295936866",
        -- Garou M1s
        "rbxassetid://13532562418",
        "rbxassetid://13532600125",
        "rbxassetid://13532604085",
        "rbxassetid://13294471966",
        -- Sonic M1s
        "rbxassetid://13370310513",
        "rbxassetid://13390230973",
        "rbxassetid://13378751717",
        "rbxassetid://13378708199",
        -- Metal Bat M1s
        "rbxassetid://14004222985",
        "rbxassetid://13997092940",
        "rbxassetid://14001963401",
        "rbxassetid://14136436157",
        -- Atomic M1s
        "rbxassetid://15259161390",
        "rbxassetid://15240216931",
        "rbxassetid://15240176873",
        "rbxassetid://15162694192",
        -- Universal
        "rbxassetid://10503381238", -- this is a lie this is uppercut
        "rbxassetid://10479335397", -- i lied again this is front dash
    }
    while true do
        for i,v in pairs(workspace.Live:GetChildren()) do
            if features.AutoBlock == true and v:FindFirstChild("HumanoidRootPart") and v:FindFirstChild("Humanoid") and v.Name ~= game.Players.LocalPlayer.Name and game.Players.LocalPlayer.Character.HumanoidRootPart then
                for i,track in pairs(v.Humanoid:GetPlayingAnimationTracks()) do
                    local distance = (game.Players.LocalPlayer.Character.HumanoidRootPart.Position - v.HumanoidRootPart.Position).Magnitude
                    if table.find(lmbs, track.Animation.AnimationId) and distance < 15 then
                        if features.AutoCounter == true and game.Players.LocalPlayer.Backpack:FindFirstChild("Prey's Peril") then
                            local args = {
                                [1] = {
                                    ["Goal"] = "Console Move",
                                    ["Tool"] = game:GetService("Players").LocalPlayer.Backpack:WaitForChild("Flowing Water")
                                }
                            }
                            game:GetService("Players").LocalPlayer.Character.Communicate:FireServer(unpack(args))
                        elseif features.AutoCounter == true and game.Players.LocalPlayer.Backpack:FindFirstChild("Split Second Counter") then
                            local args = {
                                [1] = {
                                    ["Goal"] = "Console Move",
                                    ["Tool"] = game:GetService("Players").LocalPlayer.Backpack:WaitForChild("Split Second Counter")
                                }
                            }
                            game:GetService("Players").LocalPlayer.Character.Communicate:FireServer(unpack(args))
                        end
                        local args = {
                            [1] = {
                                ["Goal"] = "KeyPress",
                                ["Key"] = Enum.KeyCode.F
                            }
                        }
                        game:GetService("Players").LocalPlayer.Character.Communicate:FireServer(unpack(args))
                        wait(0.1)
                        local args = {
                            [1] = {
                                ["Goal"] = "KeyRelease",
                                ["Key"] = Enum.KeyCode.F
                            }
                        }
                        game:GetService("Players").LocalPlayer.Character.Communicate:FireServer(unpack(args))
                    end
                end
            end
        end
        task.wait()
    end
end)


local Toggle = Tabs.Main:AddToggle("WalkFling", {Title = "Walk Fling", Default = false })
Toggle:OnChanged(function(bool)
    features.GojoInfinity = bool
end)

spawn(function()
    local hrp, c, vel, movel = nil, nil, nil, 0.1
    while true do
        game:GetService("RunService").Heartbeat:Wait()
        if features.GojoInfinity == true then
            local lp = game.Players.LocalPlayer
            while features.GojoInfinity == true and not (c and c.Parent and hrp and hrp.Parent) do
                game:GetService("RunService").Heartbeat:Wait()
                c = lp.Character
                hrp = c:FindFirstChild("HumanoidRootPart") or c:FindFirstChild("Torso") or c:FindFirstChild("UpperTorso")
            end
            if features.GojoInfinity == true then
                vel = hrp.Velocity
                hrp.Velocity = vel * 9999999 + Vector3.new(0, 9999999, 0)
                game:GetService("RunService").RenderStepped:Wait()
                if c and c.Parent and hrp and hrp.Parent then
                    hrp.Velocity = vel
                end
                game:GetService("RunService").Stepped:Wait()
                if c and c.Parent and hrp and hrp.Parent then
                    hrp.Velocity = vel + Vector3.new(0, movel, 0)
                    movel = movel * -1
                end
            end
        end
    end
end)

local Toggle = Tabs.Main:AddToggle("StreakNotifier", {Title = "Streak Notifier", Default = false })
Toggle:OnChanged(function(bool)
    features.StreakNotifier = bool
end)

spawn(function()
    local function DoStreakCheck()
        repeat task.wait() until game.Players.LocalPlayer.Character:GetAttribute("CurrentStreak")
        print('got streak att')
        if features.StreakNotifier == true then
            game.StarterGui:SetCore("SendNotification",  {
                Title = "Streak Notifier";
                Text = "Your current streak is now " .. game.Players.LocalPlayer.Character:GetAttribute("CurrentStreak");
                Icon = "rbxassetid://12740550073";
                Duration = 10;
            })
        end
        game.Players.LocalPlayer.Character:GetAttributeChangedSignal("CurrentStreak"):Connect(function(streak)
            local streak = game.Players.LocalPlayer.Character:GetAttribute("CurrentStreak")
            if streak < 0 and features.StreakNotifier == true then
                game.StarterGui:SetCore("SendNotification",  {
                    Title = "Streak Notifier";
                    Text = "You died!";
                    Icon = "rbxassetid://12740550073";
                    Duration = 10;
                })
            elseif streak > 0 and features.StreakNotifier == true then
                game.StarterGui:SetCore("SendNotification",  {
                    Title = "Streak Notifier";
                    Text = "Your current streak is now " .. game.Players.LocalPlayer.Character:GetAttribute("CurrentStreak");
                    Icon = "rbxassetid://12740550073";
                    Duration = 10;
                })
            end
        end)
    end

    DoStreakCheck()
    game.Players.LocalPlayer.CharacterAdded:connect(function()
        DoStreakCheck()
    end)
end)

local Toggle = Tabs.Main:AddToggle("CharacterOffset", {Title = "Character Offset", Default = false })
Toggle:OnChanged(function(bool)
    features.CharacterOffset = bool
end)

spawn(function()
    while wait() do
        if features.CharacterOffset == true then
            local id = "rbxassetid://13633468484"
            local anim = Instance.new("Animation")
            anim.AnimationId = id
            loaded = game.Players.LocalPlayer.Character.Humanoid.Animator:LoadAnimation(anim)
            loaded.Priority = Enum.AnimationPriority.Action4
            loaded:Play()
            loaded.TimePosition = 4
            loaded:AdjustSpeed(0)   
            workspace.CurrentCamera.CameraSubject = game.Players.LocalPlayer.Character.HumanoidRootPart
        end
    end
end)


local Toggle = Tabs.Main:AddToggle("RemoveAnim", {Title = "No Animations", Default = false })
Toggle:OnChanged(function(bool)
    features.RemoveAnim = bool
end)

spawn(function()
    while true do
        if features.RemoveAnim == true then
            for i,track in pairs(game.Players.LocalPlayer.Character.Humanoid.Animator:GetPlayingAnimationTracks()) do
                track:Stop()
            end
        end
        task.wait()
    end
end)

-- Keybinds

local Toggle = Tabs.Keybinds:AddToggle("AnimeTeleportation", {Title = "Anime Teleportation | Toggle", Default = false })
Toggle:OnChanged(function(bool)
    features.AnimeTeleportation = bool
end)

local Toggle = Tabs.Keybinds:AddToggle("AimbotBlock", {Title = "Aimbot Block | Toggle", Default = false })
Toggle:OnChanged(function(bool)
    features.AimbotBlock = bool
end)

local Toggle = Tabs.Keybinds:AddToggle("GhostMode", {Title = "Ghost Mode | Toggle", Default = false })
Toggle:OnChanged(function(bool)
    features.GhostMode = bool
end)

local Keybind = Tabs.Keybinds:CreateKeybind("TeleportKeybind", {
    Title = "Anime Teleportation",
    Mode = "Toggle", -- Always, Toggle, Hold
    Default = 'T', -- String as the name of the keybind (MB1, MB2 for mouse buttons)

    -- Occurs when the keybind is clicked, Value is `true`/`false`
    Callback = function(Value)
        if features.AnimeTeleportation == true then
            local function invisiblething(num)
                for i,v in pairs(game.Players.LocalPlayer.Character:GetDescendants()) do
                    if v:IsA("Part") or v:IsA("BasePart") then
                        if v.Name ~= "HumanoidRootPart" and not v.Name:match("Hitbox_") then
                            v.Transparency = num
                        end
                    end
                end
            end
            local tpthing = game.ReplicatedStorage.Resources.KJEffects.tpthing:Clone()
            tpthing.Parent = game.Players.LocalPlayer.Character.HumanoidRootPart
            tpthing.Enabled = true
            IYMouse = game.Players.LocalPlayer:GetMouse()
            local Char = game.Players.LocalPlayer.Character
            local HRP = Char and Char:FindFirstChild("HumanoidRootPart")
            if not Char or not HRP then
                return warn("Failed to find HumanoidRootPart")
            end
            invisiblething(1)
            HRP.CFrame = CFrame.new(IYMouse.Hit.X, IYMouse.Hit.Y + 3, IYMouse.Hit.Z, select(4, HRP.CFrame:components()))
            for i = 1,1 do
                tpthing:Emit(15)
            end
            local pchar = game.Players.LocalPlayer.Character
            local AnimationId = "15957361339"
            local Anim = Instance.new("Animation")
            Anim.AnimationId = "rbxassetid://"..AnimationId
            local k = pchar:FindFirstChildOfClass('Humanoid'):LoadAnimation(Anim)
            k:Play()
            wait(0.1)
            invisiblething(0)
            tpthing:Destroy()
        end
    end,

    -- Occurs when the keybind itself is changed, `New` is a KeyCode Enum OR a UserInputType Enum
    ChangedCallback = function(New)
        notify("Teleportation Keybind changed: " .. tostring(New))
    end
})

local BlockKeybind = Tabs.Keybinds:CreateKeybind("AimbotBlockKeybind", {
    Title = "Aimbot Block | Hold",
    Mode = "Hold", -- Always, Toggle, Hold
    Default = 'F', -- String as the name of the keybind (MB1, MB2 for mouse buttons)

    -- Occurs when the keybind is clicked, Value is `true`/`false`
    Callback = function(Value)
        print("Fired")
    end,

    -- Occurs when the keybind itself is changed, `New` is a KeyCode Enum OR a UserInputType Enum
    ChangedCallback = function(New)
        notify("Teleportation Keybind changed: " .. tostring(New))
    end
})

spawn(function()
    while true do
        if BlockKeybind:GetState() == true and features.AimbotBlock == true then
            local IsTyping = nil
            local RunService = game:GetService('RunService')
            local ReplicatedStorage = game:GetService('ReplicatedStorage')
            local UserInputService = game:GetService('UserInputService')
            local PlayerService, Player, Camera: Camera = game:GetService('Players'), game:GetService('Players').LocalPlayer, workspace.CurrentCamera
            local Character, RootPart: Part, Humanoid: Humanoid = Player.Character, Player.Character:WaitForChild('HumanoidRootPart'), Player.Character:WaitForChild('Humanoid')
            local _MaxDist = 30
            local function RefreshPlayers()
                local Users, Honored = {}, nil
                for _, v in pairs(PlayerService:GetPlayers()) do
                    if v ~= Player and v.Character then
                        local Distance = (RootPart.Position - v.Character:WaitForChild('HumanoidRootPart', 2).Position).Magnitude
                        table.insert(Users, tostring(v))
                        Users[v] = Distance
                    end
                end
                
                if not Users then return nil end
                for plr, dist in pairs(Users) do
                    if typeof(dist) ~= "string" then
                        if dist < _MaxDist then
                            if not Honored then Honored = game:GetService('Players')[tostring(plr)]
                            elseif dist <= (RootPart.Position - Honored.Character:WaitForChild('HumanoidRootPart', 2).Position).Magnitude then
                                Honored = game:GetService('Players')[tostring(plr)]
                            end
                        end
                    end
                end

                return Honored
            end
            UserInputService.InputBegan:Connect(function(_,E)
                if E then IsTyping = true end
                IsTyping = nil
            end)
            local function AimStabilizer(vChar, offset, l)
                local percent = math.clamp(l, 0, 1)
                if not Character:FindFirstChild('Ragdoll') then
                    Humanoid.AutoRotate = false
                    pcall(function(_,_)
                        local Vector
                        if UserInputService:IsKeyDown(Enum.KeyCode.C) then
                            Vector = Vector3.new(vChar.Position.X, vChar.Position.Y, vChar.Position.Z) + ((vChar.Velocity/offset) * percent)
                        else
                            Vector = Vector3.new(vChar.Position.X, RootPart.Position.Y, vChar.Position.Z) + (Vector3.new(vChar.Velocity.X, 0, vChar.Velocity.Z)/offset)
                        end
                        RootPart.CFrame = CFrame.lookAt(RootPart.CFrame.Position, Vector)
                    end)
                end
            end
            local Selected = RefreshPlayers()
            if Selected and not IsTyping then
                AimStabilizer(Selected.Character:WaitForChild("HumanoidRootPart"), 3.125, ((RootPart.Position - Selected.Character:WaitForChild('HumanoidRootPart').Position).Magnitude)/10)
            end
        end
        task.wait()
    end
end)

local player = game.Players.LocalPlayer
local screenGuisWithResetOnSpawn = {}
local originalCharacter
local ghostCharacter
local lastPosition
local function disableResetOnSpawn()
    local playerGui = player:FindFirstChildWhichIsA("PlayerGui")
    if playerGui then
        for _, object in ipairs(playerGui:GetChildren()) do
            if object:IsA("ScreenGui") and object.ResetOnSpawn then
                screenGuisWithResetOnSpawn[#screenGuisWithResetOnSpawn + 1] = object
                object.ResetOnSpawn = false
            end
        end
    end
end
local function enableResetOnSpawn()
    for _, screenGui in ipairs(screenGuisWithResetOnSpawn) do
        screenGui.ResetOnSpawn = true
    end
    table.clear(screenGuisWithResetOnSpawn)
end
local function activateGhost()
    local character = player.Character
    local humanoid
    if character ~= nil then
        humanoid = character:FindFirstChildWhichIsA("Humanoid")
    end
    if not character or not humanoid then
        error("Character or Humanoid is null")
    end
    character.Archivable = true
    ghostCharacter = character:Clone()
    character.Archivable = false
    local rootPart = character:FindFirstChild("HumanoidRootPart")
    if not rootPart then
        error("rootPart is null")
    end
    lastPosition = rootPart.CFrame
    originalCharacter = character
    local ghostHumanoid = ghostCharacter:FindFirstChildWhichIsA("Humanoid")
    ghostHumanoid.WalkSpeed = 30
    for _, child in ipairs(ghostCharacter:GetDescendants()) do
        if child:IsA("BasePart") then
            child.Transparency = 0.5
        end
    end
    local ghostAnim = ghostCharacter:FindFirstChild("Animate")
    if ghostAnim ~= nil then
        print('found')
        task.spawn(function()
            wait(0.5)
            ghostAnim.Enabled = false
            ghostAnim.Enabled = true
        end)
    end
    disableResetOnSpawn()
    ghostCharacter.Parent = character.Parent
    player.Character = ghostCharacter
    Workspace.CurrentCamera.CameraSubject = ghostHumanoid
    enableResetOnSpawn()
    local handle
    handle = humanoid.Died:Connect(function()
        handle:Disconnect()
        deactivate()
    end)
end

local function deactivateGhost()
    if not originalCharacter or not ghostCharacter then
        return nil
    end
    local rootPart = originalCharacter:FindFirstChild("HumanoidRootPart")
    local ghostRootPart = ghostCharacter:FindFirstChild("HumanoidRootPart")
    if not ghostRootPart then
        error("ghost rootPart is null")
    end
    local currentPosition = ghostRootPart.CFrame

    ghostCharacter:Destroy()
    local humanoid = originalCharacter:FindFirstChildWhichIsA("Humanoid")
    if not humanoid then
        error("ghost rootPart is null")
    end
    if humanoid ~= nil then
        local playingtracks = humanoid:GetPlayingAnimationTracks()
        local deez = function(track)
            return track:Stop()
        end
        for i, v in ipairs(playingtracks) do
            deez(v, i - 1, playingtracks)
        end
    end
    local position = currentPosition or lastPosition
    if rootPart then
        rootPart.CFrame = position
    end
    local oldcameraframe = workspace.CurrentCamera.CFrame
    disableResetOnSpawn()
    player.Character = originalCharacter
    Workspace.CurrentCamera.CameraSubject = humanoid
    enableResetOnSpawn()
    workspace.CurrentCamera.CFrame = oldcameraframe
    originalCharacter = nil
    ghostCharacter = nil
    lastPosition = nil
end


local Keybind = Tabs.Keybinds:CreateKeybind("TeleportKeybind", {
    Title = 'Ghost Mode Keybind',
    Mode = "Toggle", -- Always, Toggle, Hold
    Default = 'X', -- String as the name of the keybind (MB1, MB2 for mouse buttons)

    -- Occurs when the keybind is clicked, Value is `true`/`false`
    Callback = function(Value)
        if features.GhostMode == true and not ghostCharacter then
            activateGhost()
        elseif features.GhostMode == true and ghostCharacter then
            deactivateGhost()
        end
    end,

    -- Occurs when the keybind itself is changed, `New` is a KeyCode Enum OR a UserInputType Enum
    ChangedCallback = function(New)
        notify("Teleportation Keybind changed: " .. tostring(New))
    end
})

