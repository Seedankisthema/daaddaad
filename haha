spawn(function()
    local ongoingMoves = {}
    while true do
        for i,v in pairs(workspace.Live:GetChildren()) do
            if v:FindFirstChild("Humanoid") and v.Humanoid:FindFirstChild("Animator") and v.Name ~= game.Players.LocalPlayer.Name then
                for _,track in pairs(v.Humanoid.Animator:GetPlayingAnimationTracks()) do
                    if track.Animation.AnimationId:match("11365563255") and not table.find(ongoingMoves, v.Name) and features.AntiTF == true then
                        task.spawn(function()
                            table.insert(ongoingMoves, v.Name)
                            local theplayer = game.Players[v.Name]
                            local thumb = game.Players:GetUserThumbnailAsync(theplayer.UserId, Enum.ThumbnailType.HeadShot, Enum.ThumbnailSize.Size420x420)
                            local highlight = Instance.new("Highlight")
                            highlight.Name = "AlertHighlight3"
                            highlight.Parent = v
                            highlight.FillColor = Color3.fromRGB(255,0,0)
                            local enemyroot = v.HumanoidRootPart
                            game.StarterGui:SetCore("SendNotification",  {
                                Title = "Saitama Ultimate Move Notifier";
                                Text = theplayer.DisplayName .. "'s table flip has been detected.";
                                Icon = thumb;
                                Duration = 10;
                            })
                            game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = enemyroot.CFrame * CFrame.new(0,0,50)
                            wait(10)
                            print('move done')
                            highlight:Destroy()
                            for i,inst in pairs(ongoingMoves) do
                                if inst == tostring(v.Name) then
                                    table.remove(ongoingMoves, i)
                                    print('found')
                                end
                            end
                            print(unpack(ongoingMoves))
                        end)
                    end
                    if track.Animation.AnimationId:match("12983333733") and not table.find(ongoingMoves, v.Name) and features.AntiSP == true then
                        task.spawn(function()
                            table.insert(ongoingMoves, v.Name)
                            local theplayer = game.Players[v.Name]
                            local thumb = game.Players:GetUserThumbnailAsync(theplayer.UserId, Enum.ThumbnailType.HeadShot, Enum.ThumbnailSize.Size420x420)
                            local highlight = Instance.new("Highlight")
                            highlight.Name = "AlertHighlight3"
                            highlight.Parent = v
                            highlight.FillColor = Color3.fromRGB(255,0,0)
                            local enemyroot = v.HumanoidRootPart
                            game.StarterGui:SetCore("SendNotification",  {
                                Title = "Saitama Ultimate Move Notifier";
                                Text = theplayer.DisplayName .. "'s serious punch has been detected.";
                                Icon = thumb;
                                Duration = 13;
                            })
                            game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = enemyroot.CFrame * CFrame.new(0,0,50)
                            wait(13)
                            print('move done')
                            highlight:Destroy()
                            for i,inst in pairs(ongoingMoves) do
                                if inst == tostring(v.Name) then
                                    table.remove(ongoingMoves, i)
                                end
                            end
                        end)
                    end
                    if track.Animation.AnimationId:match("13927612951") and not table.find(ongoingMoves, v.Name) and features.AntiOM == true then
                        task.spawn(function()
                            if (game.Players.LocalPlayer.Character.HumanoidRootPart.Position - v.HumanoidRootPart.Position).Magnitude > 160 then
                                print('safe zone')
                                return
                            else
                                print('hell nah')
                            end
                            table.insert(ongoingMoves, v.Name)
                            local theplayer = game.Players[v.Name]
                            local thumb = game.Players:GetUserThumbnailAsync(theplayer.UserId, Enum.ThumbnailType.HeadShot, Enum.ThumbnailSize.Size420x420)
                            local highlight = Instance.new("Highlight")
                            highlight.Name = "AlertHighlight3"
                            highlight.Parent = v
                            highlight.FillColor = Color3.fromRGB(255,0,0)
                            local enemyroot = v.HumanoidRootPart
                            game.StarterGui:SetCore("SendNotification",  {
                                Title = "Saitama Ultimate Move Notifier";
                                Text = theplayer.DisplayName .. "'s omnidirectional punch has been detected.";
                                Icon = thumb;
                                Duration = 4;
                            })
                            game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = enemyroot.CFrame * CFrame.new(0,0,50)
                            wait(5)
                            print('move done')
                            highlight:Destroy()
                            for i,inst in pairs(ongoingMoves) do
                                if inst == tostring(v.Name) then
                                    table.remove(ongoingMoves, i)
                                end
                            end
                        end)
                    end
                    if track.Animation.AnimationId:match("13146710762") and not table.find(ongoingMoves, v.Name) and features.AntiINC  then
                        task.spawn(function()
                            table.insert(ongoingMoves, v.Name)
                            local theplayer = game.Players[v.Name]
                            local thumb = game.Players:GetUserThumbnailAsync(theplayer.UserId, Enum.ThumbnailType.HeadShot, Enum.ThumbnailSize.Size420x420)
                            local highlight = Instance.new("Highlight")
                            highlight.Name = "AlertHighlight3"
                            highlight.Parent = v
                            highlight.FillColor = Color3.fromRGB(255,0,0)
                            local enemyroot = v.HumanoidRootPart
                            game.StarterGui:SetCore("SendNotification",  {
                                Title = "Genos Ultimate Move Notifier";
                                Text = theplayer.DisplayName .. "'s incinerate has been detected.";
                                Icon = thumb;
                                Duration = 9;
                            })
                            wait(2.2)
                            game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = enemyroot.CFrame * CFrame.new(0,0,50)
                            wait(8.8)
                            print('move done')
                            highlight:Destroy()
                            for i,inst in pairs(ongoingMoves) do
                                if inst == tostring(v.Name) then
                                    table.remove(ongoingMoves, i)
                                end
                            end
                        end)
                    end
                end
            end
        end    
        for i,v in pairs(workspace.Live:GetChildren()) do
            if v.Name ~= game.Players.LocalPlayer.Name and v:GetAttribute("Ulted") == true and v:GetAttribute("Character") == "Bald" and not v:FindFirstChild("AlertHighlight2") and not v:FindFirstChild("AlertHighlight") and features.SaitUltAlert == true then
                local theplayer = game.Players[v.Name]
                local thumbType = Enum.ThumbnailType.HeadShot
                local thumbSize = Enum.ThumbnailSize.Size420x420
                local thumb = game.Players:GetUserThumbnailAsync(theplayer.UserId, thumbType, thumbSize)
                local highlight = Instance.new("Highlight")
                highlight.Name = "AlertHighlight2"
                highlight.Parent = v
                highlight.FillColor = Color3.fromRGB(255,255,0)
                game.StarterGui:SetCore("SendNotification",  {
                    Title = "Saitama Ultimate Notifier";
                    Text = theplayer.DisplayName .. " used saitama ultimate. Beware.";
                    Icon = thumb;
                    Duration = 10;
                })
                spawn(function()
                    v:GetAttributeChangedSignal("Ulted"):Wait()
                    highlight:Destroy()
                end)
            end
            if v.Name ~= game.Players.LocalPlayer.Name and v:FindFirstChild("Counter") and not v:FindFirstChild("AlertHighlight") and features.SaitUltAlert == true then
                local theplayer = game.Players[v.Name]
                local thumbType = Enum.ThumbnailType.HeadShot
                local thumbSize = Enum.ThumbnailSize.Size420x420
                local thumb = game.Players:GetUserThumbnailAsync(theplayer.UserId, thumbType, thumbSize)
                local highlight = Instance.new("Highlight")
                highlight.Name = "AlertHighlight"
                highlight.Parent = v
                game.StarterGui:SetCore("SendNotification",  {
                    Title = "Saitama Ultimate Notifier";
                    Text = theplayer.DisplayName .. " used death counter. Beware.";
                    Icon = thumb;
                    Duration = 10;
                })
                spawn(function()
                    v.Counter:GetPropertyChangedSignal("Parent"):Wait()
                    highlight:Destroy()
                end)
            end
        end
        game:GetService("RunService").Heartbeat:Wait()
    end
end)

-- Tools --

Tabs.Tools:AddButton({
    Title = "Teleport Tool",
    Description = "Makes you teleport to your mouse location",
    Callback = function()
        IYMouse = game.Players.LocalPlayer:GetMouse()
        local TpTool = Instance.new("Tool")
        TpTool.Name = "Teleport Tool"
        TpTool.RequiresHandle = false
        TpTool.Parent = game.Players.LocalPlayer.Backpack
        TpTool.Activated:Connect(function()
            local Char = game.Players.LocalPlayer.Character
            local HRP = Char and Char:FindFirstChild("HumanoidRootPart")
            if not Char or not HRP then
                return warn("Failed to find HumanoidRootPart")
            end
            HRP.CFrame = CFrame.new(IYMouse.Hit.X, IYMouse.Hit.Y + 3, IYMouse.Hit.Z, select(4, HRP.CFrame:components()))
        end)
    end
})

Tabs.Tools:AddButton({
    Title = "Fake Prey's Peril",
    Description = "Makes you do a fake prey's peril",
    Callback = function()
        local backpack = game.Players.LocalPlayer.Backpack
        local char = game.Players.LocalPlayer.Character
        local lp = game.Players.LocalPlayer
        local slowed = true -- turn on if you want realistic stun

        print("Fake Preys Peril Executed")

        if not backpack:FindFirstChild("Fake Prey's Peril") then
            local NewTool = Instance.new("Tool", game.Players.LocalPlayer)
            NewTool.Name = "Fake Prey's Peril"
            NewTool.Parent = backpack
        end

        game.Players.LocalPlayer.CharacterAdded:connect(function(v)
            print("im so fucking pissed")
            repeat wait() until #game.Players.LocalPlayer.Backpack:GetChildren() <= 4
            wait(0.5)
            local NewTool = Instance.new("Tool", game.Players.LocalPlayer)
            NewTool.Name = "Fake Prey's Peril"
            NewTool.Parent = game.Players.LocalPlayer.Backpack
        end)

        spawn(function()
            while true do
                if game.Players.LocalPlayer.Character:FindFirstChild("Fake Prey's Peril") and not game.Players.LocalPlayer.Character:FindFirstChild("FakePreysPeril") then
                    local NewInstance = Instance.new("Accessory", game.Players.LocalPlayer.Character)
                    NewInstance.Name = "FakePreysPeril"
                    if slowed == true then
                                local NewInst = Instance.new("Accessory", game.Players.LocalPlayer.Character)
                                NewInst.Name = "Slowed"
                    end
                    local AnimationId = "12351854556"
                    local Anim = Instance.new("Animation")
                    Anim.AnimationId = "rbxassetid://"..AnimationId
                    local k = game.Players.LocalPlayer.Character:FindFirstChildOfClass('Humanoid'):LoadAnimation(Anim)
                    k:Play()
                    wait(0.25)
                    k:AdjustSpeed(0.2)
                    wait(0.2)
                    k:AdjustSpeed(1)
                    wait(0.5)
                    if slowed == true then
                        game.Players.LocalPlayer.Character:FindFirstChild("Slowed"):Destroy()
                    end
                    if not game.Players.LocalPlayer.Character:FindFirstChild("Fake Prey's Peril") then
                        print('nigher')
                    else
                        game.Players.LocalPlayer.Character:FindFirstChild("Fake Prey's Peril").Parent = game.Players.LocalPlayer.Backpack
                    end
                    wait(0.5)
                    NewInstance:Destroy()
                end
                task.wait()
            end
        end)
    end
})

