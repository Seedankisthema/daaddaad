Tabs.Misc:AddInput("Input", {
    Title = "Custom Ultimate Name",
    Default = "FFLAGS EXPANSION",
    Placeholder = "FFLAGS EXPANSION",
    Numeric = false, -- Only allows numbers
    Finished = false, -- Only calls callback when you press enter
    Callback = function(Value)
        ultname = Value
    end
})

Tabs.Misc:AddInput("Input", {
    Title = "Steal Custom Cape",
    Default = "",
    Placeholder = "Put the display name of the person to steal their cape",
    Numeric = false, -- Only allows numbers
    Finished = true, -- Only calls callback when you press enter
    Callback = function(Value)
        for i,v in pairs(game.Players:GetChildren()) do
            if v ~= game.Players.LocalPlayer and v.DisplayName:lower():match(Value:lower()) then
                if v.Character:FindFirstChild("Custom Cape") then
                    local imp = v.Character["Custom Cape"]["Plane.002"]
                    print(tostring(imp.BrickColor) .. " | " .. tostring(imp.Color) .. " | " .. tostring(imp.Decal.Texture))
                    print("Set id to clipboard.")
                    notify(tostring(imp.BrickColor) .. " | " .. tostring(imp.Color) .. " | " .. tostring(imp.Decal.Texture))
                    notify("Set id to clipboard.")
                    setclipboard(imp.Decal.Texture)
                else
                    notify(v.DisplayName .. " (" .. v.Name .. ") does not have custom cape equipped.")
                end
            end
        end
    end
})

Tabs.Misc:AddInput("Input", {
    Title = "UserId / Character Name",
    Default = "327764075",
    Placeholder = "327764075",
    Numeric = false, -- Only allows numbers
    Finished = true, -- Only calls callback when you press enter
    Callback = function(Value)
        features.DisguiseChar = Value
        notify("Set Disguise ID to " .. Value )
    end
})

local function Disguisechar(char)
    local Disguise = {Enabled = true}
    local DisguiseId = features.DisguiseChar
    local DisguiseDescription
    task.spawn(function()
        if not char then return end
        local hum = char:WaitForChild("Humanoid", 9e9)
        char:WaitForChild("Head", 9e9)
        local DisguiseDescription
        if DisguiseDescription == nil then
            local suc = false
            repeat
                suc = pcall(function()
                    DisguiseDescription = game.Players:GetHumanoidDescriptionFromUserId(DisguiseId)
                end)
                if suc then break end
                task.wait(1)
            until suc or (not Disguise.Enabled)
        end
        print('1')
        if (not Disguise.Enabled) then return end
        local desc = hum:WaitForChild("HumanoidDescription", 2) or {HeightScale = 1, SetEmotes = function() end, SetEquippedEmotes = function() end}
        DisguiseDescription.HeightScale = desc.HeightScale
        char.Archivable = true
        local Disguiseclone = char:Clone()
        Disguiseclone.Name = "Disguisechar"
        Disguiseclone.Parent = workspace
        print("2")
        for i,v in pairs(Disguiseclone:GetChildren()) do
            if v:IsA("Accessory") or v:IsA("ShirtGraphic") or v:IsA("Shirt") or v:IsA("Pants") then
                v:Destroy()
            end
        end
        if not Disguiseclone:FindFirstChildWhichIsA("Humanoid") then
            Disguiseclone:Destroy()
            return
        end
        print('3')
        Disguiseclone.Humanoid:ApplyDescriptionClientServer(DisguiseDescription)
        for i,v in pairs(char:GetChildren()) do
            if v:IsA("Accessory") or v:IsA("ShirtGraphic") or v:IsA("Shirt") or v:IsA("Pants") or v:IsA("BodyColors") or v:IsA("Folder") or v:IsA("Model") then
                v:Destroy()
            end
        end
        print('4')
        char.ChildAdded:Connect(function(v)
            if ((v:IsA("Accessory") and #v:GetChildren() < 0) or v:IsA("ShirtGraphic") or v:IsA("Shirt") or v:IsA("Pants") or v:IsA("BodyColors")) and v:GetAttribute("Disguise") == nil then
                print(v.Name)
                repeat task.wait() v:Destroy() until v.Parent ~= game.Players.LocalPlayer.Character
            end
        end)
        print('5')
        for i,v in pairs(Disguiseclone:WaitForChild("Animate"):GetChildren()) do
            v:SetAttribute("Disguise", true)
            if not char:FindFirstChild("Animate") then return end
            local real = char.Animate:FindFirstChild(v.Name)
            if v:IsA("StringValue") and real then
                real.Parent = game.ReplicatedStorage
                v.Parent = char.Animate
            end
        end
        print('6')
        for i,v in pairs(Disguiseclone:GetChildren()) do
            v:SetAttribute("Disguise", true)
            if v:IsA("Accessory") then
                for i2,v2 in pairs(v:GetDescendants()) do
                    if v2:IsA("Weld") and v2.Part1 then
                        v2.Part1 = char[v2.Part1.Name]
                    end
                end
                v.Parent = char
            elseif v:IsA("ShirtGraphic") or v:IsA("Shirt") or v:IsA("Pants") or v:IsA("BodyColors") then
                v.Parent = char
            elseif v.Name == "Head" and v:FindFirstChild("Mesh") then
                if char.Head:FindFirstChild("Mesh") then
                    char.Head.Mesh:Destroy()
                end
                v.Mesh.Parent = char.Head
            elseif v.Name == "Head" and char.Head:IsA("MeshPart") then
                char.Head.MeshId = v.MeshId
            end
        end
        print('7')
        local localface = char:FindFirstChild("face", true)
        local cloneface = Disguiseclone:FindFirstChild("face", true)
        if localface and cloneface then localface.Parent = game.ReplicatedStorage cloneface.Parent = char.Head end
        Disguiseclone:Destroy()
    end)
end

Tabs.Misc:AddParagraph({
    Title = "Disclaimer",
    Content = "LordHeaven is the default avatar.",
    TitleAlignment = "Middle",
    ContentAlignment = Enum.TextXAlignment.Center
})

Tabs.Misc:AddButton({
    Title = "Disguise Character",
    Description = "",
    Callback = function()
        if features.DisguiseExec == false then
            Disguisechar(game.Players.LocalPlayer.Character)
            game.Players.LocalPlayer.CharacterAdded:connect(function()
                Disguisechar(game.Players.LocalPlayer.Character)
            end)
            features.DisguiseExec = true
        else
            notify("Disguise is already executed. It autochanges depending on the user id.")
        end
    end
})


