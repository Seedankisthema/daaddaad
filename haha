Toggle:OnChanged(function(bool)
    features.InvOM = bool
end)

task.spawn(function()
    local tableflip = false
    local seriouspunch = false
    local omni = false
    local ult = false
    while true do
        if game.Players.LocalPlayer.Character:FindFirstChild("Humanoid") then
            for i,track in pairs(game.Players.LocalPlayer.Character.Humanoid.Animator:GetPlayingAnimationTracks()) do -- Loop through the table
                if track.Animation.AnimationId:match("11365563255") and features.InvTF == true and tableflip == false then
                    tableflip = true 
                    workspace.FallenPartsDestroyHeight = "-nan"
                    track:Stop()
                    if features.InvTFHP == true then
                        game.ReplicatedStorage.DefaultChatSystemChatEvents.SayMessageRequest:FireServer("Nine ropes.. Polarized Light..", "All")
                        task.spawn(function()
                            wait(2)
                            game.ReplicatedStorage.DefaultChatSystemChatEvents.SayMessageRequest:FireServer("Crow and Declaration.. Between Front and Back..・Ao", "All")
                        end)
                    end
                    local oldcframe = game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame
                    local oldsubject = workspace.CurrentCamera.CameraSubject
                    local rootPart = game.Players.LocalPlayer.Character.HumanoidRootPart
                    local viewpart = Instance.new("Part", workspace)
                    viewpart.CFrame = oldcframe
                    viewpart.Anchored = true
                    viewpart.CanCollide = false
                    viewpart.Transparency = 1
                    workspace.CurrentCamera.CameraSubject = viewpart
                    rootPart.CFrame = CFrame.new(-999999999999999999,-999999999999999999,-999999999999999999)
                    wait(4)
                    if features.InvTFHP == true then
                        game.ReplicatedStorage.DefaultChatSystemChatEvents.SayMessageRequest:FireServer("Hollow Technique, 「Purple」", "All")
                        local pchar = game.Players.LocalPlayer.Character
                        local AnimationId = "13071982935"
                        local Anim = Instance.new("Animation")
                        Anim.AnimationId = "rbxassetid://"..AnimationId
                        local k = pchar:FindFirstChildOfClass('Humanoid'):LoadAnimation(Anim)
                        k:Play(0.2)
                        k.TimePosition = 3
                        wait(0.35)
                        k:AdjustSpeed(0)
                        wait(0.7)
                        k:AdjustSpeed(1)
                    end
                    rootPart.CFrame = oldcframe
                    workspace.CurrentCamera.CameraSubject = oldsubject
                    viewpart:Destroy()
                    wait(10)
                    tableflip = false
                    print('table flip gone :(')
                end
                if track.Animation.AnimationId:match("12983333733") and features.InvSP == true and seriouspunch == false then
                    seriouspunch = true 
                    game.Players.LocalPlayer.Character.Humanoid.AutoRotate = false
                    game.Players.LocalPlayer.Character.HumanoidRootPart.Anchored = true
                    track:Stop()
                    wait(10)
                    game.Players.LocalPlayer.Character.Humanoid.AutoRotate = true
                    seriouspunch = false
                end
                if track.Animation.AnimationId:match("13927612951") and features.InvOM == true and omni == false then
                    omni = true 
                    track:Stop()
                    wait(10)
                    omni = false
                end
                if track.Animation.AnimationId:match("12447707844") and features.InvULT == true and ult == false then
                    ult = true 
                    track:Stop()
                    local root = game.Players.LocalPlayer.Character.HumanoidRootPart
                    local oldc = game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame
                    root.CFrame = CFrame.new(-999999999999999999,-999999999999999999,-999999999999999999)
                    wait(3)
                    root.CFrame = oldc
                    ult = false
                end
            end
        end
        game:GetService("RunService").Heartbeat:Wait()
    end
end)

