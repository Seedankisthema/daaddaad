    Teleports = Window:AddTab({ Title = "Teleports", Icon = "map" }),
    Misc = Window:AddTab({ Title = "Miscellaneous", Icon = "ellipsis" }),
    Settings = Window:AddTab({ Title = "Settings", Icon = "settings" })
}
